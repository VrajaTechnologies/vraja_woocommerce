<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <record id="woocommerce_export_product_category_wizard" model="ir.ui.view">
        <field name="name">woocommerce.export.product.category.wizard</field>
        <field name="model">woocommerce.export.product.category</field>
        <field name="arch" type="xml">
            <form string="Export product Category Form">
                <group>
                    <field name="woocommerce_instance_id"/>
                </group>
                <footer>
                    <button string="Export" type="object" name="export_product_category" class="btn-primary"/>
                    <button string="Cancel" special="cancel" class="btn-secondary"/>
                </footer>
            </form>
        </field>
    </record>

    <record id="action_woocommerce_export_product_category_wizard" model="ir.actions.act_window">
        <field name="name">Woocommerce export product category Wizard</field>
        <field name="res_model">woocommerce.export.product.category</field>
        <field name="view_mode">form</field>
        <field name="target">new</field>
    </record>
</odoo>